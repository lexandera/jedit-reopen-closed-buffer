<project name="ReopenClosedBuffer" default="build">
  <description>
    This is an ant build.xml file for building the ReopenClosedBuffer plugin for jEdit.
  </description>

  <property file="build.properties"/>
  <property file="../build.properties"/>
  <property name="src.dir" value="." />
  <import file="${build.support}/plugin-build.xml" />

  <!-- Extra files that should be included in the jar -->
  <selector id="packageFiles">
    <or>
        <filename name="*.txt" />
    </or>
  </selector>
</project>
